{
  "title": "P6m Auth0 Extension",
  "name": "p6m-auth0-extension",
  "version": "0.1.0",
  "preVersion": "0.1.0",
  "author": "P6m",
  "useHashName": false,
  "description": "P6m Auth Extension",
  "type": "application",
  "category": "end_user",
  "logoUrl": "https://cdn.auth0.com/extensions/auth0-sso-dashboard/assets/logo.svg",
  "initialUrlPath": "/",
  "repository": "https://github.com/p6m-dev/auth0-extension",
  "keywords": ["p6m", "auth0", "extension"],
  "auth0": {
    "createClient": true,
    "scopes": "read:clients"
  },
  "secrets": {
    "EXTENSION_CLIENT_ID": {
      "description": "Client ID of the application you created in the Auth0 Management Dashboard",
      "required": true
    },
    "AUTH0_CUSTOM_DOMAIN": {
      "description": "Custom domain",
      "example": "example.com",
      "required": false,
      "former": "AUTH0_ISSUER_DOMAIN"
    }
  }
}
